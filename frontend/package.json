{
  "name": "cbrn-spa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate:api": "openapi-generator-cli generate -i ../api/api_docs.yaml -g typescript-angular -o src/app/core/api/oa-gen",
    "resolve:api": "openapi-generator-cli generate -i ../backend/public/docs/api_docs.yaml -g openapi-yaml -o ../backend/resolved_.yaml",
    "generate:api-client": "ng-openapi-gen --config ng-open-api-gen.config.json",
    "generate:api-server": "openapi-generator-cli generate -i ../api/api_docs.yaml -g julia-server -o ../api/CbrnAlertAPI/ --additional-properties=packageName=CbrnAlertAPI --additional-properties=exportModels=false",
    "generator:version": "openapi-generator-cli version",
    "setoaversion": "openapi-generator-cli version-manager set 6.3.0",
    "generate:all": "npm run generate:api-client && npm run generate:api-server",
    "build": "npm run generate:all && npm run ng build -- --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^8.0.0",
    "@angular/animations": "^16.1.2",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^16.1.2",
    "@angular/compiler": "^16.1.2",
    "@angular/core": "^16.1.2",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "^16.1.2",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^16.1.2",
    "@angular/platform-browser-dynamic": "^16.1.2",
    "@angular/router": "^16.1.2",
    "@asymmetrik/ngx-leaflet": "^13.0.2",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "~1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@geoman-io/leaflet-geoman-free": "^2.13.0",
    "@ngneat/forms-manager": "^2.5.0",
    "@ngneat/reactive-forms": "^5.0.2",
    "@ngxs/store": "^3.8.1",
    "@ungap/structured-clone": "^1.0.1",
    "chroma-js": "^2.4.2",
    "dayjs": "^1.10.6",
    "geojson": "^0.5.0",
    "georaster": "^1.5.6",
    "georaster-layer-for-leaflet": "^3.10.0",
    "immer": "^9.0.6",
    "leaflet": "^1.8.0",
    "leaflet-draw": "^1.0.4",
    "leaflet.heat": "^0.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.1",
    "@angular/cli": "^16.2.4",
    "@angular/compiler-cli": "^16.1.2",
    "@ngxs/devtools-plugin": "^3.8.1",
    "@ngxs/logger-plugin": "^3.8.1",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/chroma-js": "^2.1.4",
    "@types/geojson": "^7946.0.7",
    "@types/jasmine": "~3.6.0",
    "@types/leaflet": "^1.7.0",
    "@types/node": "^12.20.55",
    "@types/ungap__structured-clone": "^0.3.0",
    "autoprefixer": "^10.4.7",
    "codelyzer": "^0.0.28",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-openapi-gen": "^0.24.0",
    "postcss": "^8.4.13",
    "protractor": "~7.0.0",
    "tailwindcss": "^2.2.19",
    "tslint": "~6.1.0",
    "typescript": "~5.1.3"
  }
